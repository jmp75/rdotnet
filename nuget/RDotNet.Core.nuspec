    <language>en-US</language>
	
	<dependencies>
		<group targetFramework="netstandard2.0" >
			<dependency id="NETStandard.Library" version="2.0.0"/>
			<dependency id="DynamicInterop" version="0.9.1" />
			<dependency id="Microsoft.Win32.Registry" version="4.4.0" />
		</group>
		
		<group targetFramework="net46" >
			<dependency id="DynamicInterop" version="0.8.1" />
		</group>
		
		<group targetFramework="net40" >
			<dependency id="DynamicInterop" version="0.7.4" />
		</group>
    </dependencies>
  </metadata>
  <files>
  
   <file src="..\build\RCore.NET\release\netstandard2.0\RDotNet.NativeLibrary.Core.dll" target="lib\netstandard2.0"/>
